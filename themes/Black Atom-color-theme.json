    "editor.wordHighlightBackground": "#7a7a7a63",
    "editor.wordHighlightStrongBackground": "#7a7a7a63",
    "editor.stackFrameHighlightBackground": "#7a7a7a63",
    "editor.focusedStackFrameHighlightBackground": "#7a7a7a63",

    "editorIndentGuide.activeBackground1": "#383838",
